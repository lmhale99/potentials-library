        "description": {
            "citation": {
                "document-type": "journal",
                "title": "Embedded-atom potential for an accurate thermodynamic description of the iron-chromium system",
                "author": [
                    {
                        "given-name": "S.M.",
                        "surname": "Eich"
                    },
                    {
                        "given-name": "D.",
                        "surname": "Beinke"
