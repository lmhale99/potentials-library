        "description": {
            "citation": {
                "document-type": "journal", 
                "title": "Interatomic potentials for ternary Nb - Ti - Al alloys", 
                "author": [
                    {
                        "given-name": "D.", 
                        "surname": "Farkas"
                    }, 
                    {
                        "given-name": "C.", 
                        "surname": "Jones"
